pipeline {
    agent any

    stages {
        stage('Process Batch') {
            steps {
                script {
                    def accountBatch = params.ACCOUNT_BATCH.collect { it.account_id } 

                    def branches = [:]
                    accountBatch.each { account ->
                        branches[account] = {
                            echo "Processing account: $account"
                            sleep time: 60, unit: 'SECONDS'

                        }
                    }

                    parallel branches
                }
            }
        }
    }
}
